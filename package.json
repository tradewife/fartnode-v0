{
  "name": "fartnode-workspace",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.1",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "lint": "echo \"(add eslint later)\"",
    "typecheck": "tsc -b --pretty",
    "test": "vitest run --reporter=basic || jest -w 1",
    "build": "pnpm -r run build",
    "dev:web": "pnpm -C apps/web dev",
    "dev:worker": "pnpm -C apps/action-worker dev"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
